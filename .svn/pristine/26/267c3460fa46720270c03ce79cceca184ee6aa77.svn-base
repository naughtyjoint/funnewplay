  `Pla_email` char(50) COLLATE utf8_unicode_ci NOT NULL,
  `Pla_tel` char(20) COLLATE utf8_unicode_ci NOT NULL,
  `Pla_cell` char(20) COLLATE utf8_unicode_ci DEFAULT NULL,
  `Pla_add` char(225) COLLATE utf8_unicode_ci NOT NULL,
  `Pla_people` int(11) NOT NULL,
  `Pla_price` int(11) unsigned NOT NULL,
  `Pla_des` text COLLATE utf8_unicode_ci,
  `Pla_web` varchar(225) COLLATE utf8_unicode_ci DEFAULT NULL
) ENGINE=InnoDB AUTO_INCREMENT=23 DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='場地';

