--
-- 資料表結構 `facility`
--

CREATE TABLE IF NOT EXISTS `facility` (
  `Facility_id` int(20) unsigned NOT NULL COMMENT 'P.K.',
  `Pla_id` int(20) unsigned NOT NULL COMMENT 'F.K.',
  `Facility_name` char(20) COLLATE utf8_unicode_ci NOT NULL,
  `Facility_des` text COLLATE utf8_unicode_ci,
  `Facility_optime` time DEFAULT NULL,
  `Facility_cltime` time DEFAULT NULL,
  `Facility_people` int(11) NOT NULL,
  `Facility_price` int(11) DEFAULT NULL
) ENGINE=InnoDB AUTO_INCREMENT=62 DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='設施';

--
-- 資料表的匯出資料 `facility`
--

INSERT INTO `facility` (`Facility_id`, `Pla_id`, `Facility_name`, `Facility_des`, `Facility_optime`, `Facility_cltime`, `Facility_people`, `Facility_price`) VALUES
(1, 1, '遊客中心', 'Write something~', '08:00:00', '22:00:00', 400, 0),
(2, 6, '攀岩山訓', '在險惡的環境中求生可以說是攀岩山訓的目的，因此來到這裡，不妨鼓起勇氣，練習行走位於山谷間的雙索、三索突擊吊橋，訓練出過人的膽識與勇氣。\r\n', '11:00:00', '18:00:00', 50, 700),
(3, 6, '漆彈場', '漆彈..........在國外已經是一種平常休閒娛樂之一\r\n在國內，跳跳幾年前引進漆彈活動\r\n看慣戰爭片的您，是否很想體會叢林野戰的震撼教育，跳跳農場聘請專業教官帶領您享受真槍實彈的快感和刺激', '10:00:00', '17:00:00', 80, 1300),
(4, 6, '露天咖啡座', '如果您玩攀岩、漆彈或是賞蝶賞鳥後，可以到俯視曾文水庫的露天咖啡區，來杯特有高山香醇咖啡或飲品，在大自然的懷抱中，攬望三角南山翠綠山峰，四周時而山嵐雲霧飄渺，時而清晰直望漁人舢舨。', '08:00:00', '06:00:00', 40, 120),
(5, 4, '露營區', '仙湖提供簡易的露營場地，獨立山頂戲水池旁下側草地 （但地面有斜度喔！)，熱水衛浴及電力24小時提供。\r\n因為風景，讓露營愛好者們選擇在這裡落腳，但我們想先告知有意願的露營愛好者們，這並非一個專業、有著許多完善設施的露營場地。\r\n採現場排位制，故免預約，建議電話詢問當日場地使用狀況。 \r\n費用以人數計算，不再加收門票，成人每位280元、兒童每位250元，此費用已含露營場地、設施及野炊清潔費', '00:00:00', '12:00:00', 700, 1100),
(6, 4, '休閒農場', '由專人帶領體驗當季的農作生產:「從花茶、香包的製作、龍眼蜂蜜養育、產地水果的採收。到莊園咖啡農的扮演及傳統桂圓乾的生產體驗。」並在生活中實踐。餐桌上的主角，是親自找尋摘採的野菜食材，並以當季素材DIY下午茶，享受在地農夫才能體會的美好生活。', '05:00:00', '16:00:00', 1000, 500),
(7, 5, '露營區', '園區等級：名列國際三星級頂級露營區，並通過各項安全檢查且榮獲觀光局考核頒發之「特優」獎項。\r\n營區容納：2,000人次。\r\n營區配置：分上、下二區，可同時容納兩所學校同時使用。\r\n露營器材：帳篷、毛毯、睡袋、炊具、桌椅等樣樣齊全', '00:00:00', '22:00:00', 1100, 600),
(8, 5, '農場生態', '☆ 地形：月世界、掘鑿曲流、頁狀河階。\r\n☆ 地質：青灰岩、化石、卵石。\r\n☆ 水質源：冷水坑、曾文溪及水泉眼。\r\n☆ 動物：肉牛、馬匹、山兔、松鼠、水鳥、魚類。\r\n☆ 植物：熱帶草原、草原、香花、藥草、古樹林。\r\n☆ 視覺及田園景觀，豊富而多變化，較其它風景區更\r\n    具主題特色。', '09:00:00', '19:00:00', 2000, 700),
(9, 5, '動物園區', 'Write something~', '00:00:00', '17:00:00', 400, 100),
(10, 7, '露營烤肉', '露營區(場內可容納100頂營帳，600人大型營火晚會)', '00:00:00', '21:00:00', 600, 250),
(11, 7, '大雅山餐廳', '豪華特餐：（每桌 4500）四色冷盤、八寶魚翅、筍乾封肉、酥炸雙拼、情人果蝦球、山蘇丁香魚、芒果蘆筍燴、麻油雞米糕、梅子三杯雞、鹽酥蜂蛹、砂鍋魚頭湯、燒酒雞湯◎附白飯及水果', '09:00:00', '20:00:00', 550, 1100),
(12, 7, '森林步道', '漫步在充滿芬多精的林間步道，享受森林浴。', '11:00:00', '12:00:00', 900, 0),
(13, 8, '藝術畫廊', '在農場研習中心的二樓大廳，有著一條藝術畫廊.，裡面收藏了眾多畫家與雕刻加的名作，讓您除了接觸大自然之外，還可以接觸到更多藝術品，讓您可以好好的欣賞自然美景與人文創作的交會', '10:00:00', '18:00:00', 150, 50),
(14, 8, '親子游泳池', '本農場內設有親子游泳池，位於休閒會館後方，是您親子戲水的最佳場所，如果您覺得海邊的人多擁擠，在農場內山光水色的景致中，也可以帶給您戲水的快樂，炎炎夏日中，是您最好的消暑方式。', '05:00:00', '19:00:00', 300, 120),
(15, 8, '池畔咖啡廳', '停雲潭旁的湖畔咖啡座，是讓您可以輕鬆度過悠閒時光的地方，您可以在此啜飲美味的飲料，欣賞湖面上的天空倒影，過境的候鳥，也會在湖岸旁與您一起度過這美好的時間。', '07:00:00', '20:00:00', 75, 100),
(16, 8, '大型停車場', '在本農場的大門設有大型停車場供您使用，讓您不管是團體或是個人前來，都不用煩惱停車的問題。', '00:00:00', '12:00:00', 1000, 500),
(17, 9, '親水步道', '位於台灣島戲水區旁，除台灣島區域可以戲水，親水步道引水而入，創造一區域可以用腳踩戲水，塑造出另一戲水空間。', '00:00:00', '08:00:00', 150, 0),
(18, 9, '動物島', '動物島湖泊區有7座小型島嶼，各住著不同種類的動物，各種動物在小島上悠閒漫步供遊客欣賞，還有會唱歌的長臂猿，等待每次遊客經過時，小唱一曲博得遊客歡心，湖內的回饋亭還有各色錦鯉可觀賞、飼養。', '11:00:00', '17:00:00', 500, 0),
(19, 9, '楓咖啡', '位於五甲區，外頭有著日式大觀園造景景觀，有加拿大紅楓與造型真柏，可觀賞漂亮景色!\r\n\r\n楓咖啡裡頭可品嘗台南著名的東山咖啡及各式各樣的簡餐餐點，非常優閒自栽喔!', '09:00:00', '20:00:00', 50, 120),
(20, 9, '珍奇茶花區', '為農場特色之一，一般茶花在過濁水溪以南比較難生長，所以農場特在此區精心栽培茶花，號稱在南部生長最佳的茶花園。', '09:00:00', '21:00:00', 150, 0),
(21, 9, '兒童樂園', '位於泡茶區旁的兒童樂園，可提供孩童們遊憩的場所!', '09:00:00', '17:00:00', 400, 0),
(22, 9, '忘憂沙灘', '忘憂沙灘位於諾亞方舟大舞台區與台灣島之間...提供小朋友最佳戲沙場地...平常除了可以戲沙...亦提供各幼稚園...國小學生們沙灘尋寶的活動', '08:00:00', '22:00:00', 250, 0),
(23, 9, '古奇木區', '古奇木區內有許多在台灣屬於珍貴稀奇或具高度觀賞性的樹種，非常值得在進入農場時一遊，此步道連接湖畔小木屋與常綠餐廳，於晚間閒步至餐廳時，還可享受由不同樹木所散發出的芬多精與花香。', '10:00:00', '17:00:00', 250, 0),
(24, 9, '生態保育鳥園', '鳥園裡有飼養50幾種鳥類，並可在週邊湖泊與森林內發現約100多種鳥類，總合約150幾種，其中有目前世界上最大的鴿子、帶面具的雞、會說話的鸚鵡……等等各種珍奇鳥獸，更包含了幾種台灣難以見到的保育類珍禽。', '09:00:00', '17:00:00', 200, 50),
(25, 10, '大峽谷', '我是『懶惰的農夫』，既不種茶也不種檳榔，光種一些無收成的雜木；不養豬、也沒有養雞，只養幾隻小樹蛙，與一堆的黑甲虫~^^，大家都笑我江爸爸是一個憨農夫，我倒認為我較像是梅山大峽谷裡面，花、鳥、蟲、魚的奴才……哈哈哈……您不相信嗎？那歡迎您有空來梅山大峽谷走走看看…', '00:00:00', '23:00:00', 700, 0),
(26, 10, '裕之廬', '【裕之廬】內之設備有：四人房x4間+2人帳蓬x2頂(最多可住五戶)+\r\n樓上客廳x1+樓下客廳x2+衛浴x2間+廚房x1+餐廳x1+中庭x1(有休閒吊籃可觀瀑布美景)+前庭(可烤肉)+DIY教室x1', '00:00:00', '12:00:00', 120, 1220),
(27, 10, '宗瑩石頭屋', '	\r\n\r\n◎此木屋有2人房*1間+8人房*1間＝可住10人\r\n◎一樓:客廳+餐廳+廚房+衛浴 2樓:兩間房間\r\n◎住一晚+早、晚兩餐+導覽，每人1000元\r\n◎純住宿一晚每人700元(可自備食材來烹煮)\r\n◎吉普車曠野之旅含一宿二餐，每人只要1500元\r\n◎整棟包租7000元/天（純住宿不含餐，2可自備食材來烹煮）', '00:00:00', '12:00:00', 30, 1350),
(28, 10, '原始摩登一號木屋', '\r\n◎此木屋有4人房*2間+和式房*1+閣樓兒童房＝可住16人\r\n◎內有四房+客廳+餐廳+廚房+衛浴\r\n◎住一晚+早、晚兩餐+導覽，每人1000元\r\n◎純住宿一晚每人700元(可自備食材來烹煮)\r\n◎吉普車曠野之旅含一宿二餐，每人只要1500元\r\n◎整棟包租8000元/天（純住宿不含餐，可自備食材來烹煮）', '00:00:00', '12:00:00', 100, 1000),
(29, 10, '歐風木屋(歐風廬)', '◎此木屋有2人房*1 + 3人房*1＝可住5人\r\n◎內有一房+半樓＋客廳+衛浴\r\n◎住一晚+早、晚兩餐+導覽，每人1000元\r\n◎純住宿一晚每人700元(可自備食材來烹煮)\r\n◎吉普車曠野之旅含一宿二餐，每人只要1500元\r\n◎整棟包租3500元/天（純住宿不含餐，可自備食材來烹煮）', '00:00:00', '12:00:00', 40, 1200),
(30, 11, '爬蟲類生態園區', '爬蟲類生態園區', '10:00:00', '18:00:00', 250, 0),
(31, 11, '哺乳類生態園區', '哺乳類生態園區', '00:00:00', '12:00:00', 250, 0),
(32, 11, '鳥禽類生態園區', '鳥禽類生態園區', '00:00:00', '12:00:00', 250, 0),
(33, 12, '漂漂河邊草', '環繞園區四周的人工河道，總長約700公尺，遊客可藉著游泳圈的浮力，以及特殊設計的水流緩慢漂行，環繞一週約20分鐘，閉目靜聽大海傳來的聲音，同時又可享受自在悠閒的南國風光。', '11:00:00', '22:00:00', 200, 0),
(34, 13, '遊客中心', '遊客服務中心', '09:00:00', '18:00:00', 200, 0),
(36, 1, '第一烤肉屋', '第一烤肉屋位於農場主要道路', '00:00:00', '23:59:00', 200, 0),
(37, 1, '第二烤肉屋', '位置清幽有獨立洗手間旁邊即是茶園', '00:00:00', '23:59:00', 160, 0),
(38, 1, '第三烤肉屋', 'Write something~', '00:00:00', '23:59:00', 260, 0),
(39, 1, '大草地', 'Write something~', '00:00:00', '23:59:00', 400, 0),
(40, 1, '第一戶外教室', '有遮雨', '00:00:00', '00:00:00', 200, 0),
(41, 1, '遊客中心', '所有問題的解決地方', '08:00:00', '22:00:00', 600, 0),
(42, 2, '遊客中心', 'Write something~', '09:00:00', '18:00:00', 80, 0),
(43, 2, '小巨蛋', '遮風避雨容納人數多', '00:00:00', '23:59:00', 500, 0),
(44, 1, '第二戶外教室', '溫馨的小場地', '00:00:00', '23:59:00', 160, 20),
(45, 1, '製茶廠', '日據時代留下來的製茶廠', '00:00:00', '23:59:00', 300, 3000),
(46, 16, '生態區', '◎ 園內動、植物，請勿捕捉、並請愛護自然資源。 \r\n◎ 為保護水源及森林環境，請將垃圾及廢物分類，丟入垃圾筒內或帶離下山。 \r\n◎ 樹木、岩石、標示牌及各種設施物，請勿刻字、繪圖或損壞等行為。 \r\n◎ 為維護場內安全，請於指定地點營火、烤肉。 ', '10:30:00', '15:30:00', 50, 200),
(47, 17, '瞭望台', '香格里拉休閒農場的特色之一，\r\n就是到處可以發現造型不同的涼亭，\r\n我們體貼每位旅人一路上的辛勞，\r\n所以建議您不妨進來小歇坐坐，\r\n臨吹清風，\r\n盡賞山下大片美景！', '10:00:00', '18:00:00', 25, 0),
(48, 17, '觀光果園', '隨著四季變化的植物蔬果，\r\n您可以恣意穿梭在這結實累累的果園之中，\r\n滿載一籃的鮮甜水果與一臉歡愉笑顏，\r\n這是回歸農村生活的簡單娛樂！', '10:00:00', '18:00:00', 50, 30),
(49, 17, '農場步道', '來到山間林裡，\r\n適合暢流一身的汗水，\r\n農場裡輕畫幾條不刻意的信步小道，\r\n若只是隨意走走也可以，\r\n最好再配合幾次深度的呼吸，\r\n如此便能清醒進、煩憂出！', '10:00:00', '18:00:00', 80, 0),
(50, 18, '餐廳', '夏天吃火鍋？有沒搞錯啊？\r\n沒沒沒！在布農部落，現在除了是鳳梨採收盛季，也是有機蔬果豐收之時，\r\n布農部落的小火鍋提供了當天現採的有機蔬果：\r\n南瓜、地瓜、絲瓜、長豆、地瓜葉、紅莧菜、Ａ菜、芥菜、秋葵、玉米。。。等\r\n加上自製的豚骨湯頭、羊肉爐，真是美味極了；\r\n如果您想來個魚頭，布農部落也不會讓您失望！\r\n火鍋煮著時您可以先來盤鳳梨，甜中微酸的滋味可是會讓您胃口大開哦\r\n～ 吃完再挖一碗冰淇淋～～\r\n～幸福啊～', '10:00:00', '20:00:00', 400, 0),
(51, 18, '表演場', '這裡，沒有豪華舞台，沒有華麗裝扮，只有原住民寬闊、具穿透力的嗓音，百分百的感動與震撼。\r\n\r\n部落劇場的表演是我們傳達布農文化精華的方式，每一段樂舞都蘊含了布農的習俗以及熱情團結的個性。我們更盼望透過表演與練習，將布農的傳統樂舞保留下來，以免這些代代相傳的文化祭儀在現代化的過程中，連部落裡的布農孩子也遺忘了。\r\n\r\n尤其布農族的八部合音，布農語叫pasibutbut，是祈求小米豐收所進行的祭天儀式。沒有指揮、沒有樂譜、泛音的半音階唱法，每每令人震撼。\r\n\r\n部落劇場曾受邀至美加、日本演出，深受好評。\r\n\r\n沒看到演出，等於沒來過。', '10:00:00', '17:00:00', 500, 100),
(52, 18, '農產品展售中心', '台東旅遊最有特色的伴手禮就在布農部落休閒農場！\r\n農特產中心陳列展售物品\r\n1.台灣原住民以及布農族的傳統編織品。\r\n2.結合現代應用設計的各式創意編織品：手機袋、零錢包、背包、大包、服飾...等。\r\n3.自產ＣＡＳ認證竹炭製品，可：淨水、除濕、除臭...等。\r\n4.自製第一道萃取精油，蒸餾下來，水油分離，原油直接分裝，天然無添加。\r\n5.自製天然精油手工香皂：檜木皂、香茅皂、樟腦皂、竹醋皂、竹炭皂；每款均添加蘆薈,洗臉、沐浴皆可。\r\n6.自產農特加工產品：阿嬤釀的小米酒、手工炭火鹽炒花生、竹炭花生(甜) 、各式季節果汁果醬、酵素、果醋、干貝辣椒醬、有機蔬菜、小米、紫米等。\r\n現場提供試吃服務，歡迎大家品嚐。', '08:00:00', '17:00:00', 100, 0),
(53, 19, '初鹿牧場', '全國最大坡地牧場，周邊奶製品銷售及騎馬、滑草等活動。', '09:00:00', '17:00:00', 150, 100),
(54, 19, '餐廳', '不論是中式精緻合菜，BBQ，簡餐，任君挑選！', '06:00:00', '23:00:00', 80, 200),
(55, 19, '直排輪鞋溜冰場  複合式活動廣場', '注意事項：有夜間照明設備直排輪鞋租借需求', '07:00:00', '22:00:00', 50, 0),
(56, 20, '烹飪教室', '新鮮水果利用高張溶液原理，動手做成好吃水果蜜餞', '10:00:00', '17:00:00', 30, 150),
(57, 20, '自然教室', '30年的鐵牛車陪伴阿輝的童年記憶，阿輝親自帶你走進水果森林', '10:00:00', '17:00:00', 100, 100),
(58, 20, '動物教室', '餵食每份10元', '10:00:00', '17:00:00', 50, 10),
(59, 22, '羊舍', '想看成群的羊兒嗎？在農場裡頭就住著一群可愛的羊咩咩，農場現有山羊13頭，採放牧式畜養，遊客來場可自由親近追逐。羊兒成群嬉戲、低頭啃食青草。羊舍構築採階段式坡道設計，可從事羊隻觀察與教學活動，配合觀察筆記、解說，是親子互動與戶外教學的良好教材。', '10:00:00', '16:00:00', 150, 50),
(60, 22, '吊床區', '本區位於行政中心旁肖楠造林地中，佔地0.2公頃，茂密的肖楠林中，涼風徐徐，躺臥吊床中擺盪身心，靜聽婉轉悅耳的鳥嗚。', '10:00:00', '17:00:00', 30, 30),
(61, 22, '果園', '農場早期遍植果樹，現僅保留少量芭樂及楊桃各約100株，提供教學、研習活動功能，為使遊客瞭解果樹栽培的過程與樂趣，本場開發小農夫筆記，做為親子及戶外教學活動之教材。', '10:00:00', '17:00:00', 100, 100);

